  "type": "module",
  "description": "hooks-admin open source management system",
  "author": {
    "name": "Hooks",
    "email": "848130454@qq.com",
    "url": "https://github.com/HalseySpicy"
  },
  "license": "MIT",
  "homepage": "https://github.com/HalseySpicy/Hooks-Admin",
  "repository": {
    "type": "git",
    "url": "git@github.com:HalseySpicy/Hooks-Admin.git"
  },
  "bugs": {
    "url": "https://github.com/HalseySpicy/Hooks-Admin/issues"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build:dev": "tsc && vite build --mode development",
    "build:test": "tsc && vite build --mode test",
    "build:pro": "tsc && vite build --mode production",
    "type:check": "tsc --noEmit --skipLibCheck",
    "preview": "npm run build:dev && vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "standard-version",
    "commit": "git add -A && czg && git push"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@reduxjs/toolkit": "^1.9.5",
    "ahooks": "^3.7.7",
    "antd": "^5.6.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "echarts": "^5.4.2",
    "i18next": "^23.0.2",
    "install": "^0.13.0",
    "js-md5": "^0.7.3",
    "nprogress": "^0.2.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.0",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.13.0",
    "react-transition-group": "^4.4.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/js-md5": "^0.7.0",
    "@types/node": "^18.16.0",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@types/react-transition-group": "^4.4.6",
    "@types/web-bluetooth": "^0.0.17",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cz-git": "^1.6.1",
    "czg": "^1.6.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^15.7.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
